y_true=[[0,1,0],[0,0,1]]
y_pred=[[0.05,0.95,0],[0.1,0.8,0.1]]
cee=tf.keras.losses.CategoricalCrossentropy()
print(cce(y_true,y_predict).numpy())
-> 1.1769392

# this will not work
y_true=[1,2]
y_pred=[[0.05,0.95,0],[0.1,0.8,0.1]]
cee=tf.keras.losses.CategoricalCrossentropy()
print(cce(y_true,y_predict).numpy())


# for this to work
y_true=[1,2]
y_pred=[[0.05,0.95,0],[0.1,0.8,0.1]]
cee=tf.keras.losses.SparseCategoricalCrossentropy()
print(cce(y_true,y_predict).numpy())
